<h1>Possible Collaborators</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><%=  user.email %></td>
        		
        <td><%= "Add to Collaborators" %></td>
   
      </tr>
    <% end %>
  </tbody>
</table>

